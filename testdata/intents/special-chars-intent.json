{
  "apiVersion": "nephoran.com/v1",
  "kind": "NetworkIntent",
  "metadata": {
    "name": "special-chars-test-デプロイメント-测试",
    "namespace": "test-ñámespace-with-spëcial-chàrs",
    "labels": {
      "test.type": "special-characters",
